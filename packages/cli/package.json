{
  "name": "@vibecheck/cli",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vibecheck": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "vibecheck": "node ./dist/index.js"
  },
  "dependencies": {
    "@vibecheck/policy": "workspace:*",
    "@vibecheck/schema": "workspace:*",
    "commander": "^12.1.0",
    "fast-glob": "^3.3.2",
    "ts-morph": "^24.0.0"
  },
  "devDependencies": {
    "vitest": "^2.1.8",
    "@types/node": "^22.10.2"
  }
}
